{
  "name": "gymhub-backend-update-a",
  "version": "1.1.1",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --exec node --loader ts-node/esm src/index.ts",
    "build": "prisma generate && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "node --loader ts-node/esm prisma/seed.ts",
    "prisma:validate": "prisma validate"
  },
  "dependencies": {
    "@prisma/client": "5.17.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "prisma": "5.17.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "nodemon": "^3.1.4"
  }
}
